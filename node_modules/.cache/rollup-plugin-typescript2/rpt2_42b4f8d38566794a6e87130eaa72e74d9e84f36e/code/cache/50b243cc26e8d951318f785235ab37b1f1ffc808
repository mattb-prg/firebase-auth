remove = function (key) {\r\n        this.storage.removeItem(key);\r\n        return Promise.resolve();\r\n    };\r\n    Object.defineProperty(BrowserPersistenceClass.prototype, \"storage\", {\r\n        get: function () {\r\n            return this.storageRetriever();\r\n        },\r\n        enumerable: false,\r\n        configurable: true\r\n    });\r\n    return BrowserPersistenceClass;\r\n}());\r\nexport { BrowserPersistenceClass };\r\n//# sourceMappingURL=browser.js.map","references":["/home/matt/Desktop/Projects/Freelance/Damon Chen/firebase-package/packages/auth/src/core/persistence/index.ts"],"map":"{\"version\":3,\"file\":\"browser.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../../src/platform_browser/persistence/browser.ts\"],\"names\":[],\"mappings\":\"AAAA;;;;;;;;;;;;;;;GAeG;AAEH,OAAO,EAEL,qBAAqB,EAEtB,MAAM,wBAAwB,CAAC;AAEhC,wEAAwE;AACxE,2EAA2E;AAC3E,UAAU;AAEV;IACE,iCACqB,gBAA+B,EACzC,IAAqB;QADX,qBAAgB,GAAhB,gBAAgB,CAAe;QACzC,SAAI,GAAJ,IAAI,CAAiB;IAC7B,CAAC;IAEJ,8CAAY,GAAZ;QACE,IAAI;YACF,IAAI,CAAC,IAAI,CAAC,OAAO,EAAE;gBACjB,OAAO,OAAO,CAAC,OAAO,CAAC,KAAK,CAAC,CAAC;aAC/B;YACD,IAAI,CAAC,OAAO,CAAC,OAAO,CAAC,qBAAqB,EAAE,GAAG,CAAC,CAAC;YACjD,IAAI,CAAC,OAAO,CAAC,UAAU,CAAC,qBAAqB,CAAC,CAAC;YAC/C,OAAO,OAAO,CAAC,OAAO,CAAC,IAAI,CAAC,CAAC;SAC9B;QAAC,WAAM;YACN,OAAO,OAAO,CAAC,OAAO,CAAC,KAAK,CAAC,CAAC;SAC/B;IACH,CAAC;IAED,sCAAI,GAAJ,UAAK,GAAW,EAAE,KAAuB;QACvC,IAAI,CAAC,OAAO,CAAC,OAAO,CAAC,GAAG,EAAE,IAAI,CAAC,SAAS,CAAC,KAAK,CAAC,CAAC,CAAC;QACjD,OAAO,OAAO,CAAC,OAAO,EAAE,CAAC;IAC3B,CAAC;IAED,sCAAI,GAAJ,UAAiC,GAAW;QAC1C,IAAM,IAAI,GAAG,IAAI,CAAC,OAAO,CAAC,OAAO,CAAC,GAAG,CAAC,CAAC;QACvC,OAAO,OAAO,CAAC,OAAO,CAAC,IAAI,CAAC,CAAC,CAAC,IAAI,CAAC,KAAK,CAAC,IAAI,CAAC,CAAC,CAAC,CAAC,IAAI,CAAC,CAAC;IACzD,CAAC;IAED,yCAAO,GAAP,UAAQ,GAAW;QACjB,IAAI,CAAC,OAAO,CAAC,UAAU,CAAC,GAAG,CAAC,CAAC;QAC7B,OAAO,OAAO,CAAC,OAAO,EAAE,CAAC;IAC3B,CAAC;IAED,sBAAc,4CAAO;aAArB;YACE,OAAO,IAAI,CAAC,gBAAgB,EAAE,CAAC;QACjC,CAAC;;;OAAA;IACH,8BAAC;AAAD,CAAC,AArCD,IAqCC\"}","dts":{"name":"/home/matt/De